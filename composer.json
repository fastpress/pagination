{
  "name": "fastpress/paginator",
  "description": "Pagination library for FastPress applications",
  "type": "library",
  "keywords": ["pagination", "paginator", "php", "fastpress",  "framework"],
  "homepage": "https://github.com/fastpress/paginator",
  "support": {
      "issues": "https://github.com/fastpress/request/issues",
      "source": "https://github.com/fastpress/request"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Samson Daniel",
      "email": "samayo@pm.me"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "FastPress\\Pagination\\": "src/Pagination/"
    }
  },
  "minimum-stability": "dev",
  "scripts": {
    "test": "phpunit",
    "fix-style": "phpcbf --standard=PSR12 src/",
    "lint": "phpcs --standard=PSR12 src/",
    "analyze": "phpstan analyse src/"
},
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.6",
    "phpstan/phpstan": "^1.0"
},
  "config": {
    "optimize-autoloader": true
  }
}